<view>
<view class="card" wx:for="{{items}}">
        <view data-publish-time="{{item.time}}" bindtap="onItemClick">
            <image class="image" mode="aspectFill" src="{{item.imagePath}}"/>
            <text class="factoryName">{{item.factoryName}}</text>
            <view class="factoryDesc">{{item.factoryDesc}}</view>
        </view>
    </view>
</view>
